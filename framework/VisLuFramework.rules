<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="VisLuFramework rules"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="GLSL Shader Tool"
			DisplayName="GLSL Shader Tool"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(OutDir)\$(InputFileName)&quot;"
			Outputs="$(OutDir)\$(InputFileName)"
			FileExtensions="*.glsl"
			ExecutionDescription="Copying $(InputFileName)"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
